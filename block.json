{
	"apiVersion": 2,
	"name": "cjd-blocks/interactive-map",
	"version": "0.1.0",
	"title": "Interactive Map",
	"category": "theme",
	"icon": "location-alt",
	"description": "An interactive map that highlights certain states by filtering criteria.",
	"attributes": {
		"id": {
			"type": "string"
		},
		"filterType": {
			"type": "string",
			"default": "Category"
		},
		"filterOptions": {
			"type": "array",
			"default": []
		},
		"mapColor": {
			"type": "string"
		},
		"highlightColor": {
			"type": "string"
		}
	},
	"supports": {
		"html": false,
		"align": true
	},
	"textdomain": "interactive-map",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
